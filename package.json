    "@typescript-eslint/parser": "^2.28.0",
    "acorn": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.7.1",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.5",
