    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "acorn": "^7.3.1",
    "eslint": "^7.2.0",
    "eslint-config-alloy": "^3.7.2",
    "eslint-plugin-jest": "^23.13.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
